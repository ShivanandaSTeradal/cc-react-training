<script>
   
   function unique(arr) {
    var set= new Set(arr);
    return (Array.from(set.values()))

   }
   
   let arr = ["green", "green", "red", "red","yellow", "pink", "pink", "yellow"];
   
var values=unique(arr);
console.log("Unique values are:",unique(arr));
document.getElementById('unique(arr)').innerHTML=unique(arr);
</script>
